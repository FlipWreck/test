// ğŸ“Š INPUT PARAMETERS
var g1 = "ğŸ¯ SIGNAL SETTINGS"
signalSensitivity = input.float(1.5, "Signal Sensitivity", minval=0.5, maxval=3.0, step=0.1, group=g1)
