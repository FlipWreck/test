        tp1Distance = math.abs(currentTP1 - close) / close * 100
        tp1Text = "$" + str.tostring(currentTP1, "#.####") + " (" + str.tostring(tp1Distance, "#.##") + "%)"
        table.cell(tradingInterface, 1, 10, tp1Text, text_color=color.aqua)
        
        table.cell(tradingInterface, 0, 11, "🎯 Take Profit 2", text_color=color.white)
        tp2Distance = math.abs(currentTP2 - close) / close * 100
        tp2Text = "$" + str.tostring(currentTP2, "#.####") + " (" + str.tostring(tp2Distance, "#.##") + "%)"
        table.cell(tradingInterface, 1, 11, tp2Text, text_color=color.aqua)
        
        table.cell(tradingInterface, 0, 12, "🎯 Take Profit 3", text_color=color.white)
        tp3Distance = math.abs(currentTP3 - close) / close * 100
        tp3Text = "$" + str.tostring(currentTP3, "#.####") + " (" + str.tostring(tp3Distance, "#.##") + "%)"
        table.cell(tradingInterface, 1, 12, tp3Text, text_color=color.aqua)
        
        table.cell(tradingInterface, 0, 13, "💼 Position Size", text_color=color.white)
        table.cell(tradingInterface, 1, 13, "$" + str.tostring(currentPositionSize, "#.##"), text_color=color.yellow)
        
        table.cell(tradingInterface, 0, 14, "⚡ Leverage", text_color=color.white)
        leverageText = str.tostring(currentLeverage, "#") + "x"
        leverageColor = currentLeverage > 10 ? color.red : currentLeverage > 5 ? color.orange : color.lime
        table.cell(tradingInterface, 1, 14, leverageText, text_color=leverageColor)
        
        table.cell(tradingInterface, 0, 15, "💸 Risk Amount", text_color=color.white)
        table.cell(tradingInterface, 1, 15, "$" + str.tostring(currentRiskAmount, "#.##"), text_color=color.orange)
    
    // Strategy Scores Section
    scoreStartRow = entryActive ? 16 : not na(plannedEntryPrice) ? 13 : 6
    
    table.cell(tradingInterface, 0, scoreStartRow, "📊 STRATEGY SCORES", text_color=color.white, bgcolor=color.new(color.purple, 60))
    table.cell(tradingInterface, 1, scoreStartRow, "AI ANALYSIS", text_color=color.white, bgcolor=color.new(color.purple, 60))
    
    table.cell(tradingInterface, 0, scoreStartRow + 1, "⚡ Scalping", text_color=color.white)
    scalpColor = scalpingStrategyScore > 50 ? color.lime : scalpingStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 1, str.tostring(scalpingStrategyScore, "#"), text_color=scalpColor)
    
    table.cell(tradingInterface, 0, scoreStartRow + 2, "🚀 Breakout", text_color=color.white)
    breakoutColor = breakoutStrategyScore > 50 ? color.lime : breakoutStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 2, str.tostring(breakoutStrategyScore, "#"), text_color=breakoutColor)
    
    table.cell(tradingInterface, 0, scoreStartRow + 3, "📈 Momentum", text_color=color.white)
    momentumColor = momentumStrategyScore > 50 ? color.lime : momentumStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 3, str.tostring(momentumStrategyScore, "#"), text_color=momentumColor)
    
    table.cell(tradingInterface, 0, scoreStartRow + 4, "🔄 Reversal", text_color=color.white)
    reversalColor = reversalStrategyScore > 50 ? color.lime : reversalStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 4, str.tostring(reversalStrategyScore, "#"), text_color=reversalColor)
    
    table.cell(tradingInterface, 0, scoreStartRow + 5, "📊 MA Cross", text_color=color.white)
    maColor = maCrossoverStrategyScore > 50 ? color.lime : maCrossoverStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 5, str.tostring(maCrossoverStrategyScore, "#"), text_color=maColor)
    
    table.cell(tradingInterface, 0, scoreStartRow + 6, "📉 RSI Strategy", text_color=color.white)
    rsiColor = rsiStrategyStrategyScore > 50 ? color.lime : rsiStrategyStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 6, str.tostring(rsiStrategyStrategyScore, "#"), text_color=rsiColor)
    
    table.cell(tradingInterface, 0, scoreStartRow + 7, "📰 News Trading", text_color=color.white)
    newsColor = newsTradingStrategyScore > 50 ? color.lime : newsTradingStrategyScore > 30 ? color.yellow : color.gray
    table.cell(tradingInterface, 1, scoreStartRow + 7, str.tostring(newsTradingStrategyScore, "#"), text_color=newsColor)
    
    // Market Analysis Section
    analysisStartRow = scoreStartRow + 8
    
    table.cell(tradingInterface, 0, analysisStartRow, "🌊 MARKET ANALYSIS", text_color=color.white, bgcolor=color.new(color.navy, 60))
    table.cell(tradingInterface, 1, analysisStartRow, "LIVE DATA", text_color=color.white, bgcolor=color.new(color.navy, 60))
    
    table.cell(tradingInterface, 0, analysisStartRow + 1, "📊 RSI", text_color=color.white)
    rsiDisplayColor = rsi < 30 ? color.lime : rsi > 70 ? color.red : rsi < 40 ? color.yellow : rsi > 60 ? color.orange : color.gray
    table.cell(tradingInterface, 1, analysisStartRow + 1, str.tostring(rsi, "#.#"), text_color=rsiDisplayColor)
    
    table.cell(tradingInterface, 0, analysisStartRow + 2, "📈 Trend", text_color=color.white)
    trendText = trend == 1 ? "BULLISH" : "BEARISH"
    trendDisplayColor = trend == 1 ? color.lime : color.red
    table.cell(tradingInterface, 1, analysisStartRow + 2, trendText, text_color=trendDisplayColor)
    
    table.cell(tradingInterface, 0, analysisStartRow + 3, "🌊 Volatility", text_color=color.white)
    volText = extremeVolatility ? "EXTREME" : highVolatility ? "HIGH" : microVolatility ? "MICRO" : lowVolatility ? "LOW" : "NORMAL"
    volDisplayColor = extremeVolatility ? color.red : highVolatility ? color.orange : microVolatility ? color.yellow : color.gray
    table.cell(tradingInterface, 1, analysisStartRow + 3, volText, text_color=volDisplayColor)
    
    table.cell(tradingInterface, 0, analysisStartRow + 4, "📊 Volume", text_color=color.white)
    volumeText = extremeVolumeSpike ? "EXTREME" : megaVolumeSpike ? "MEGA" : volumeSpike ? "HIGH" : microVolumeSpike ? "MICRO" : "NORMAL"
    volumeDisplayColor = extremeVolumeSpike ? color.red : megaVolumeSpike ? color.orange : volumeSpike ? color.yellow : microVolumeSpike ? color.lime : color.gray
    table.cell(tradingInterface, 1, analysisStartRow + 4, volumeText, text_color=volumeDisplayColor)

