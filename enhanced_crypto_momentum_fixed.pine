var float currentLeverage = na
var float currentRiskAmount = na
var int currentDirection = na
var string activeSignalType = na

entryTimeoutBars = input.int(5, "Entry Expiry Bars", minval=2, maxval=20)  // Reduced timeout

// -- FIXED: Detect new setup with realistic entry prices --
setupSignal = aiLongSignal or aiShortSignal
plannedLong  = aiLongSignal and na(plannedEntryPrice)
plannedShort = aiShortSignal and na(plannedEntryPrice)

if plannedLong or plannedShort
    // FIXED: Set more realistic entry prices that can actually be reached
    entryDirection := plannedLong ? 1 : -1
    
    // Use slightly better prices that are more likely to be hit
    entryBuffer = atr * 0.1  // Small buffer for realistic entry
    plannedEntryPrice := entryDirection == 1 ? 
                         math.min(close + entryBuffer, high + (atr * 0.05)) :  // Long: slightly above current or near high
                         math.max(close - entryBuffer, low - (atr * 0.05))     // Short: slightly below current or near low
    
    setupBarIndex := bar_index
    entryActive := false
    tradeEntryPrice := na
    plannedStopLoss := calculateStopLoss(entryDirection, plannedEntryPrice)
    [plannedTP1, plannedTP2, plannedTP3] = calculateTakeProfits(entryDirection, plannedEntryPrice, plannedStopLoss)
    plannedSignalType := finalStrategy

// -- FIXED: More realistic entry triggering logic --
entryTriggered = false
if not entryActive and not na(plannedEntryPrice)
    // FIXED: Use proper price range checking for entry triggers
    if entryDirection == 1
        // Long entry: triggered when price reaches or exceeds planned entry within the bar's range
        entryTriggered := high >= plannedEntryPrice
    else
        // Short entry: triggered when price reaches or falls below planned entry within the bar's range  
        entryTriggered := low <= plannedEntryPrice
        
    if entryTriggered
        entryActive := true
        tradeEntryPrice := plannedEntryPrice  // Use planned price
        currentEntryPrice := tradeEntryPrice
        currentDirection := entryDirection
        currentStopLoss := plannedStopLoss
        currentTP1 := plannedTP1
        currentTP2 := plannedTP2
        currentTP3 := plannedTP3
        [currentPositionSize, currentLeverage, currentRiskAmount, _] = calculatePositionSize(currentDirection, currentEntryPrice, currentStopLoss)
        activeSignalType := plannedSignalType

// -- Expire setup if not triggered within N bars --
if not entryActive and not na(setupBarIndex) and (bar_index - setupBarIndex) > entryTimeoutBars
    plannedEntryPrice := na
    setupBarIndex := na
    entryDirection := na
    plannedStopLoss := na
    plannedTP1 := na
    plannedTP2 := na
    plannedTP3 := na
    plannedSignalType := na

// -- FIXED: Improved exit logic --
emergencyLongExit = rsi < 15 or (close < ema_trend * 0.95 and longTermTrend == -1)
emergencyShortExit = rsi > 85 or (close > ema_trend * 1.05 and longTermTrend == 1)

// Enhanced exit conditions with TP hits
tpHit = false
if entryActive
    if currentDirection == 1  // Long position
        tpHit := high >= currentTP1 or high >= currentTP2 or high >= currentTP3
    else  // Short position
        tpHit := low <= currentTP1 or low <= currentTP2 or low <= currentTP3

stopHit = false
if entryActive
    if currentDirection == 1  // Long position
        stopHit := low <= currentStopLoss
    else  // Short position
        stopHit := high >= currentStopLoss

closeActive = entryActive and (stopHit or emergencyLongExit or emergencyShortExit or tpHit)

if closeActive
    plannedEntryPrice := na
    setupBarIndex := na
    entryDirection := na
    plannedStopLoss := na
    plannedTP1 := na
    plannedTP2 := na
    plannedTP3 := na
    plannedSignalType := na
